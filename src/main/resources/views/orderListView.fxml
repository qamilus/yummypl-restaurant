<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.swislowski.kamil.projekt.koncowy.yummypl.restaurant.gui.controller.OrderListController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn prefWidth="112.0" text="Nazwa restauracji" />
                <TableColumn prefWidth="112.0" text="Ilość pozycji" />
                  <TableColumn prefWidth="112.0" text="Status" />
                  <TableColumn prefWidth="112.0" text="Adres dostawy" />
              </columns>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" text="Zmiana Statusu" />
                  <Button mnemonicParsing="false" text="Szczegóły zamówienia" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
